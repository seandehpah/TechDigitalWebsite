{
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/api/sendmail$",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/sendmail.js"
    },
    {
      "src": "/api/(.*)$",
      "dest": "/api/$1.js"
    },
    {
      "src": "^/(.+\\.[a-z0-9]+)$",
      "dest": "/$1"
    },
    {
      "src": "^/(.+)\\.html$",
      "dest": "/$1/"
    },
    {
      "src": "^/(.*)\\.html$",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)/$",
      "dest": "/$1.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/$1.html"
    }
  ]
}